<!--顶部-->
<header class="header">
    <a href="javascript:;" class="back"></a>
    <h2>领券中心</h2>
    <a href="javascript:;" class="rightlink" id="share"><img src="images/coupon/share.png" style="
    height: 0.33rem;
    width: 0.33rem;
    padding: 0.275rem 0rem 0.275rem 0.275rem;
"></a>
</header>

<!--中间-->
<section class="coupons_main">

    {{#each data}}
    <div class="coupons_box">
    	<div class="coupons_box_l">
    		<em>¥<b>{{PRICE}}</b></em>
            <ul><li>使用范围：{{RANGE_NAME}}</li><li>{{START_TIME}}至{{END_TIME}}</li></ul>
    	</div>
        <div class="coupons_box_c">
        </div>
        <div class="coupons_box_r" data-id="{{ACTIVE_ID}}">立即领取&nbsp;></div>
    </div>
    {{/each}}

    {{#hasnodata data}}
    <div class="coupons_box" style="font-size: 16px; text-align: center; margin-top: 20px;">
        暂无数据~
    </div>
    {{/hasnodata}}
    
    
</section>
<!--/中间-->